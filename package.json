{
  "name": "redux-logic-ssr-boilerplate",
  "version": "1.0.0",
  "description": "redux-logic-ssr-boilerplate",
  "author": "yyynnn",
  "repository": {
    "type": "git",
    "url": "https://github.com/yyynnn/redux-logic-ssr-boilerplate"
  },
  "engines": {
    "node": ">=10.5.0"
  },
  "scripts": {
    "dev:wds": "cross-env NODE_ENV=development IS_SSR=0 IS_WDS=1 webpack-dev-server --config ./webpack/wds.babel.js",
    "dev:ssroff": "npm run clean && cross-env NODE_ENV=development IS_SSR=0 nodemon --watch server --exec babel-node --inspect --extensions '.js,.jsx' src/server/index.js",
    "dev:ssr": "npm run clean && cross-env NODE_ENV=development IS_SSR=1 nodemon --watch server --exec babel-node --inspect --extensions '.js,.jsx' src/server/index.js",
    "prod": "cross-env NODE_ENV=production IS_SSR=1 node ./build/bundle.server.js",
    "prod:debug": "npm run build && cross-env NODE_ENV=development IS_SSR=1 node --inspect ./build/bundle.server.js",
    "prod:build": "npm run build && cross-env NODE_ENV=production IS_SSR=1 node ./build/bundle.server.js",
    "build": "npm run clean && npm-run-all -p build:client build:server",
    "build:client": "cross-env NODE_ENV=production IS_SSR=1 webpack --mode production --config ./webpack/prod.ts",
    "build:server": "cross-env NODE_ENV=production IS_SSR=1 webpack --mode production --config ./webpack/server.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --coverage",
    "test:u": "npm test -- -u",
    "fix-memory-limit": "cross-env LIMIT=16000 increase-memory-limit",
    "clean": "rm -rf build/*",
    "todos": "leasot --exit-nicely ./src/**/*.js ./src/**/*.jsx"
  },
  "keywords": [],
  "license": "MIT",
